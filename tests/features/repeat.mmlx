/**
 * so many ways to say the same thing
 */
repeat1:
    volume: 1..15.step(2) 1..15.step(2)

repeat2:
    volume: 1(+2)..15 1(+2)..15

repeat3:
    volume: 1..15.step(2).repeat(2)

repeat4:
    volume: [1..15].step(2).repeat(2)

repeat5:
    volume: 1(+2)..15.repeat(2)

repeat6:
    volume: [1..15.step(2)].repeat(2)

repeat7:
    volume: [1(+2)..15].repeat(2)

repeat8:
    volume: [[1..15].step(2)].repeat(2)

repeat9:
    volume: [1..15].step(2) [1..15].step(2)

repeat10:
    volume: 1 3 5 7 9 11 13 15 [1..15].step(2)

